{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "out",
  "framework": null,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_TYPESCRIPT_CONFIG_PATH": "tsconfig.deploy.json",
    "NODE_ENV": "production",
    "NEXT_TELEMETRY_DISABLED": "1"
  }
} 